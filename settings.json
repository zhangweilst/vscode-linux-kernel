    "editor.tabSize": 8,
    "C_Cpp.intelliSenseCacheSize": 51200,
    "C_Cpp.default.intelliSenseMode": "linux-gcc-x64",
    "C_Cpp.intelliSenseCachePath": "/data1/vscode-cache",
    "editor.detectIndentation": false
